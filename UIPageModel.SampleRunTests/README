
# stream 1 - not parallel
dotnet test --filter "TestCategory=Stream 1&TestCategory=Chromium" --settings .stream1
dotnet test --filter "TestCategory=Stream 1&TestCategory=Firefox" --settings .stream1
dotnet test --filter "TestCategory=Stream 1&TestCategory=Webkit" --settings .stream1

# stream 2 - parallel
dotnet test --filter "TestCategory=Stream 2&TestCategory=Chromium" --settings .stream2
dotnet test --filter "TestCategory=Stream 2&TestCategory=Firefox" --settings .stream2
dotnet test --filter "TestCategory=Stream 2&TestCategory=Webkit" --settings .stream2